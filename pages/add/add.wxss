.main {
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;  
  width: 100%;
  height: 100%;
}
.head {
    margin: auto;
    height: 93rpx;
    border-bottom: 1px solid #edecee;
}
.head-text {
    text-indent: 265rpx;
    float: left;
    font-size: 32rpx;
    line-height: 93rpx;
}
.head-finish {
    margin-left: 80px;
    line-height: 93rpx;
    font-size: 32rpx;
    color: #14b392;
}
.title {
    font-size: 28rpx;
    margin-top: 20px;
    color: #a2a3a7;
}

.icon {
  margin-top: 20px;
  width: 50px;
  height: 50px
}

.note {
    margin: auto;
    margin-top: 40px;
    width: 672rpx;
    height: 344rpx;
    background: white;
    border: 1px solid #edecee;
    border-radius: 15rpx;
}
.note textarea {
    font-size: 28rpx;
    margin-top: 20rpx;
    margin-left: 30rpx;
    color: #a2a3a7;
}